# Example dput.cf that defines the host that can be used
# with dput for uploading.

[DEFAULT]
login = lefant
method = scp
hash = md5
allow_unsigned_uploads = 1
run_lintian = 0
run_dinstall = 0
check_version = 0
scp_compress = 0
post_upload_command =
pre_upload_command =
passive_ftp = 1
default_host_main = lefant
default_host_non-us =

[lefant]
fqdn			= lefant.net
incoming		= ~/public_html/debian/incoming
login			= e
